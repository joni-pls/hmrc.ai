{
  "version": 2,
  "functions": {
    "backend/api/query_handler.py": {
      "runtime": "@vercel/python@3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/query",
      "destination": "/backend/api/query_handler.py"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ]
}