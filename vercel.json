{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/query_handler.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/query",
      "dest": "backend/api/query_handler.py",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}